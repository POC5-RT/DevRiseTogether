                "tabsetConfig" : "{\"UUID\":\"9f7c6a40-4258-4cc9-80e4-bfb27bef8e5d\",\"activeTab\":null,\"useOverflowMenu\":null,\"tabs\":[{\"UUID\":\"30ada8e6-eba2-40cd-8dc9-dfa3f4d13ff8\",\"tabName\":\"About US\",\"tabKey\":\"tab1\",\"locked\":false,\"allowGuestUser\":false},{\"UUID\":\"6ba9f197-5472-4d7f-83ce-c563b483ee18\",\"tabName\":\"Introduction\",\"tabKey\":\"tab2\",\"locked\":false,\"allowGuestUser\":false},{\"UUID\":\"35753969-fbfc-4f6b-91d5-f440d5f3aa70\",\"tabName\":\"Our mission\",\"tabKey\":\"ef25d\",\"locked\":false,\"allowGuestUser\":false},{\"UUID\":\"100a1d0c-808f-42e1-9cd2-e1ab33b8cb83\",\"tabName\":\"Meet Our Volunteers/Team\",\"tabKey\":\"7c07c\",\"locked\":false,\"allowGuestUser\":false},{\"UUID\":\"b20c36a3-904d-4bb2-9145-404a6d02115a\",\"tabName\":\"Join US\",\"tabKey\":\"f6dc0\",\"locked\":false,\"allowGuestUser\":false}]}",
                "width" : "100%"
